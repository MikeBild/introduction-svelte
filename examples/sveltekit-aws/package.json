{
  "name": "sveltekit-aws",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "dotenv -e .env svelte-kit dev",
    "build": "tsc -p ./deploy/tsconfig.json && dotenv -e .env svelte-kit build",
    "package": "svelte-kit package",
    "preview": "dotenv -e .env svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@aws-cdk/aws-dynamodb": "1.144.0",
    "@aws-cdk/core": "1.144.0",
    "@sveltejs/kit": "1.0.0-next.278",
    "@types/node": "17.0.19",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@typescript-eslint/parser": "5.12.1",
    "aws-sdk": "2.1079.0",
    "dotenv-cli": "5.0.0",
    "eslint": "8.9.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-plugin-svelte3": "3.4.0",
    "mdsvex": "0.10.5",
    "prettier": "2.5.1",
    "prettier-plugin-svelte": "2.6.0",
    "svelte": "3.46.4",
    "svelte-check": "2.4.5",
    "svelte-preprocess": "4.10.3",
    "sveltekit-adapter-aws": "2.0.10",
    "tslib": "2.3.1",
    "typescript": "4.5.5"
  },
  "type": "module"
}
